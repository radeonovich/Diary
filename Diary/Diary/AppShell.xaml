             x:Class="Diary.AppShell"
        xmlns:views="clr-namespace:Diary.Views">
    <TabBar>
        <ShellContent
            Title="Вход"
            Icon="iconLogin.png"
            ContentTemplate="{DataTemplate views:LoginPage}">
